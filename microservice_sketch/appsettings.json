{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "api_settings": {
    "memory_switch": true,
    "dataBase_switch": true,
    "nlogger_switch": true,
    "memory_catch": {
      "size_limit": 100,
      "words_limit": 100,
      "expiration": 60,
      "actions": [ "size_limit", "words_limit", "passive", "initiative" ]
    }
  }
}
